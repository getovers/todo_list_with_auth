{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "quotes": ["error", "single"],
    "indent": ["error", 2],
//    "no-trailing-spaces": "error", //заборона зайвих пробілів,
    "prefer-const": "error",
    "semi": ["error", "never"],
    "no-multiple-empty-lines": ["error", { "max": 1 }],
    "no-duplicate-imports": "error",
    "object-curly-spacing": ["error", "always"]
  }
}
